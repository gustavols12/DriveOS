generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Produto{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  cost String
  price String
   createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt

}